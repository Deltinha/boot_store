{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "object-curly-newline": ["error", {
            "ExportDeclaration": { "multiline": true }
        }],
        "import/prefer-default-export": "off"
    }
}
